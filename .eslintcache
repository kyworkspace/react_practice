는 경우에만 표시 */}\r\n        </React.Fragment>\r\n      );\r\n    }\r\n    ComponentWithError.defaultProps = {\r\n      errorMessage: defaultMessage,\r\n    };\r\n\r\n    ComponentWithError.displayName = `withError(${wrappedComponentName})`;\r\n\r\n    return withStyles(({ color }) => ({\r\n      error: {\r\n        color: color.error,\r\n      },\r\n    }))(ComponentWithError);\r\n  };\r\n}\r\n","C:\\React\\begin-react\\src\\06\\HomePageComponent.jsx",[],"C:\\React\\begin-react\\src\\06\\LoadingProvider.jsx",["293"],"C:\\React\\begin-react\\src\\06\\withLoadingContextAndKey.jsx",["294"],"import React from \"react\";\r\nimport { DEFAULT_KEY, contextPropTypes } from \"./LoadingProviderWithKey\";\r\n\r\nexport const loadingPropTypes = contextPropTypes;\r\n//공급자의 컨텍스트데이터 자료형을 가져와 loadingPropTypes에 대입\r\n\r\nexport default (contextKey = DEFAULT_KEY) => (WrappedComponent) => {\r\n  //공급자의 컨텍스트 데이터 키값을 인자로 전달받는 하이어오더 컴포넌트 제작\r\n  const { displayName, name: componentName } = WrappedComponent;\r\n  const wrappedComponentName = displayName || componentName;\r\n\r\n  function WithLoadingContext(props, context) {\r\n    const { loading, setLoading } = context[contextKey];\r\n    //키값에 맞는 컨텍스트 데이터 구조분해 할당\r\n    return (\r\n      <WrappedComponent {...props} loading={loading} setLoading={setLoading} />\r\n    );\r\n  }\r\n\r\n  WithLoadingContext.displayName = `withLoadingContext(${wrappedComponentName})`;\r\n  WithLoadingContext.contextTypes = {\r\n    [contextKey]: contextPropTypes,\r\n  };\r\n\r\n  return WithLoadingContext;\r\n};\r\n","C:\\React\\begin-react\\src\\06\\HomePageWithTwoProvider.jsx",[],"C:\\React\\begin-react\\src\\06\\createLoadingConsumer.jsx",["295","296"],"C:\\React\\begin-react\\src\\06\\ButtonWithConsumer.jsx",["297"],"import React from \"react\";\r\nimport PropTypes from \"prop-types\";\r\nimport Button from \"../04/Button\";\r\nimport createLoadingConsumer from \"./createLoadingConsumer\";\r\n\r\nconst DefaultLoadingConsumer = createLoadingConsumer();\r\nc